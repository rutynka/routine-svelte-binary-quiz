{
  "name": "routine-svelte-binary-quiz",
  "version": "0.0.2",
  "author": "ru7ynk4",
  "description": "Rutynka binary quiz - with helpers",
  "svelte": "src/index.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "files": [
    "src/index.js",
    "src/App.svelte"
  ],
  "keywords": [
    "rutynka",
    "svelte",
    "binary",
    "true-false",
    "quiz",
    "progress",
    "calendar",
    "local storage",
    "bar",
    "board",
    "helper",
    "heatmap"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --port 5000",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/svelte": "^3.0.3",
    "babel-jest": "^27.5.0",
    "jest": "^27.5.0",
    "jest-transform-stub": "^2.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-jester": "^2.3.1"
  },
  "dependencies": {
    "@rutynka/helper-bar-board": "0.1.0",
    "@rutynka/helper-progress": "^0.1.2",
    "sirv-cli": "^2.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": "svelte-jester",
      "^.+\\.js$": "babel-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(helper-bar-board))"
    ],
    "moduleFileExtensions": [
      "svelte",
      "js"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rutynka/routine-svelte-binary-quiz"
  },
  "homepage": "https://github.com/rutynka/routine-svelte-binary-quiz",
  "url": "https://rutynka.io",
  "license": "MIT"
}
